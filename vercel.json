{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ],
  "installCommand": "cd client && npm install --legacy-peer-deps",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "build"
}
